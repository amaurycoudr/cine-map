{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "cSpell.words": [
        "Allocine",
        "tmdb"
    ],
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": false
    }
}